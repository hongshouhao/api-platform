{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/administration/{url}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 48969
        }
      ],
      "UpstreamPathTemplate": "/ocelot/{url}",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "ocelot_administration",
        "AllowedScopes": [ "roles" ]
      },

      "RouteClaimsRequirement": {
        "role": "ocelot_administrator"
      }
    },
    {
      "DownstreamPathTemplate": "/v1/{url}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "192.168.34.95",
          "Port": 8500
        }
      ],
      "UpstreamPathTemplate": "/consul/{url}",
      "UpstreamHttpMethod": [ "Get", "Put" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "consul_administration",
        "AllowedScopes": [ "roles" ]
      },
      "RouteClaimsRequirement": {
        "role": "consul_administrator"
      }
    }
  ]
}
